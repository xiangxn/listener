{
    "python.defaultInterpreterPath": "venv/bin/python",
    "editor.defaultFormatter": "eeyore.yapf",
    "yapf.args": [
        "--style",
        "{column_limit:160,space_inside_brackets:false,arithmetic_precedence_indication:true,blank_line_before_nested_class_or_def:true,spaces_around_dict_delimiters:true}"
    ],
    "editor.codeActionsOnSave": {
        "source.organizeImports.yapf": "explicit"
    },
    "[go]": {
        "editor.defaultFormatter": "golang.go"
    },
    "solidity.packageDefaultDependenciesContractsDirectory": "trader/src",
    "solidity.packageDefaultDependenciesDirectory": "trader/lib",
    "editor.formatOnSave": true,
    "[solidity]": {
        "editor.defaultFormatter": "JuanBlanco.solidity"
    },
    "solidity.formatter": "forge"
}